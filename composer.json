{
  "name": "devmonk/htmlpurifier",
  "type": "library",
  "description": "Zend Framework 2 module that provides complete integration of HTMLPurifier library",
  "keywords": [
    "zf2",
    "htmlpurifier",
    "html",
    "security",
    "module",
    "malicious",
    "xss",
    "filter",
    "twig"
  ],
  "homepage": "https://github.com/kelsey9649/devmonk-htmlpurifier",
  "license": "LGPL v 2.1+",
  "require": {
    "php": "^5.6 || ^7.0",
    "ezyang/htmlpurifier": "^4.0",
    "zendframework/zend-servicemanager": ">=2.7",
    "zendframework/zend-view": "^2.0"
  },
  "require-dev": {
    "zf-commons/zfc-twig": "^1.0",
    "zendframework/zendframework": ">=2.4"
  },
  "suggest": {
    "zf-commons/zfc-twig": "Provides full twig system for zf2 applications. This package provides a 'purify' filter for twig templates."
  },
  "autoload": {
    "psr-4": {
      "DevmonkHtmlpurifier\\": "src/DevmonkHtmlpurifier/"
    }
  }
}
